{
    "name": "cortex/categories",
    "description": "Cortex Categories is a frontend layer for the polymorphic Laravel package, for category management. You can categorize any eloquent model with ease, and utilize the power of Nested Sets, and the awesomeness of Sluggable, and Translatable models out of the box.",
    "type": "cortex-module",
    "keywords": [
        "model",
        "cortex",
        "laravel",
        "eloquent",
        "category",
        "sluggable",
        "translatable",
        "categorizable",
        "polymorphic",
        "nested-set",
        "taxonomy",
        "rinvex"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-categories/issues",
        "source": "https://github.com/rinvex/cortex-categories",
        "docs": "https://github.com/rinvex/cortex-categories/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-categories/contributors"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "illuminate/auth": "^6.0.0",
        "illuminate/console": "^6.0.0",
        "illuminate/http": "^6.0.0",
        "illuminate/routing": "^6.0.0",
        "illuminate/support": "^6.0.0",
        "cortex/foundation": "^3.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3.0",
        "kalnoy/nestedset": "^5.0.0",
        "laravelcollective/html": "^6.0.0",
        "league/fractal": "^0.18.0",
        "proengsoft/laravel-jsvalidation": "^2.5.0",
        "rinvex/laravel-categories": "^3.0.0",
        "rinvex/laravel-composer": "^3.0.0",
        "rinvex/laravel-menus": "^3.0.0",
        "rinvex/laravel-support": "^3.0.0",
        "silber/bouncer": "v1.0.0-rc.6",
        "spatie/laravel-activitylog": "^3.8.0",
        "yajra/laravel-datatables-buttons": "^4.8.0",
        "yajra/laravel-datatables-fractal": "^1.5.0",
        "yajra/laravel-datatables-html": "^4.16.0",
        "yajra/laravel-datatables-oracle": "^9.6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^8.3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Categories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Categories\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Categories\\Providers\\CategoriesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
